    private void autoPauseVideo(final int pauseAt, int nonTimer) {
        //to stop on media player
/*while(vv.getCurrentPosition()==pauseAt)
            Toast.makeText(this, "stopHere!!!", Toast.LENGTH_SHORT).show();
